@model List<BooksApp.Domain.Entities.Publisher>

@{
    ViewBag.Title = "Publishers";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.First().Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.First().Country)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var publisher in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => publisher.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => publisher.Country)
                </td>
                <td></td>
            </tr>
        }
    </tbody>
</table>